#%Module 1.0
#
#  cube module for use with environment modules
#
conflict	cube
prepend-path 	PATH 		@PREFIX@/bin
prepend-path	MANPATH		@PREFIX@/share/man
prepend-path 	LD_LIBRARY_PATH @PREFIX@/@LIB@
setenv		CUBE_DIR	@PREFIX@
setenv		CUBE_HOME	@PREFIX@
setenv		CUBE_INCLUDE	@PREFIX@/include
setenv		CUBE_LIB	@PREFIX@/@LIB@
